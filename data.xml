<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- Bean Auto Wiring -->

	<bean id = "kaka" class="SetterInject.FootballPlayer" autowire = "byName">
		<constructor-arg name = "shirtNumber" value = "7"></constructor-arg>
	</bean>
	
	<bean id = "forlan" class="SetterInject.FootballPlayer" autowire = "byName">
		<constructor-arg name = "shirtNumber" value = "21"></constructor-arg>
	</bean>	
		
<!-- Bean Configuration -->

	<bean id = "predator" class = "SetterInject.Predator">
	</bean>
	
</beans>